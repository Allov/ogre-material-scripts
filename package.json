{
    "name": "ogrematerialscripts",
    "displayName": "material",
    "description": "Syntax Highlighting for Ogre Material Scripts",
    "version": "0.0.2",
    "publisher": "allov",
    "repository": {
        "type": "git",
        "url": "https://github.com/Allov/ogre-material-scripts"
    },
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "ogrematerialscripts",
            "aliases": ["Ogre Material Scripts", "ogrematerialscripts"],
            "extensions": [".compositor",".material",".program"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "ogrematerialscripts",
            "scopeName": "source.ogre",
            "path": "./syntaxes/OgreMaterialScripts.tmLanguage"
        }]
    }
}